{
  "compilerOptions": {
    "declaration": true,
    "sourceMap": true,
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "Node",
    "target": "es5",
    "module": "es2022",
    "lib": ["es2017", "dom"],
    "types": ["vite/client", "node"],
    "jsx": "react-jsx",
    "resolveJsonModule": true
  },
  "exclude": ["build", "node_modules"]
}
