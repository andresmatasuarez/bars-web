{
	"clean": [
		"tmp/",
		"wp-content/themes/bars2013/"
	],
	"dest": {
		"php": "wp-content/themes/bars2013/",
		"lib": "wp-content/themes/bars2013/js/",
		"css": "wp-content/themes/bars2013/css/",
		"img": "wp-content/themes/bars2013/images/",
		"misc": "wp-content/themes/bars2013/resources/",
		"sprites": "wp-content/themes/bars2013/images/sprites.png",
		"cssSpritesImgPath": "images/sprites.png",
		"libOutput": "wp-content/themes/bars2013/js/lib.min.js",
		"cssOutput": "wp-content/themes/bars2013/style.css"
	},

	"tmp": "tmp/",

	"src": {
		"lib": [
			"lib/fancybox/source/jquery.fancybox.js",
			"lib/jquery-form/jquery.form.js",
			"lib/jquery-validation/dist/jquery.validate.js",
			"lib/jQuery.dotdotdot/src/js/jquery.dotdotdot.js",
			"lib/imgLiquid/js/imgLiquid.js",
			"lib/jquery-focuspoint/js/jquery.focuspoint.js"
		],

		"css": [
			"css/imports.css",
			"css/links.css",
			"css/text.css",
			"css/sponsors.css",
			"css/contact.css",
			"css/main.css",
			"css/genericons/genericons.css",
			"css/slider.css",
			"css/sprites.css",
			"lib/jquery-focuspoint/css/focuspoint.css"
		],

		"js": [
			"js/bars.js",
			"js/contact.js",
			"js/slider.js"
		],

		"img": [
			"img/bars2013/*",
			"img/bars2014/*",
			"img/logo_bg.png",
			"img/auspiciantes.png",
			"img/bgpattern2.png",
			"img/bgpattern3.png",
