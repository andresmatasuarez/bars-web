			"img/hover_facebook.png",
			"img/facebook.png",
			"img/hover_flickr.png",
			"img/flickr.png",
			"img/hover_twitter.png",
			"img/twitter.png",
			"img/hover_tumblr.png",
			"img/tumblr.png",
			"img/hover_youtube.png",
			"img/youtube.png",
			"img/hover_festeringslime.png",
			"img/festeringslime.png",
			"img/footer_logo.png",
			"img/header.png",
			"img/loguito.png",
			"img/arrows.png"
		],

		"php": [
			"php/*",
			"!php/exportmovies.php"
		],

		"misc": [
			"misc/**/*"
		]
	},

	"fancybox": {
		"src" : [
			"lib/fancybox/source/jquery.fancybox.css",
			"lib/fancybox/source/blank.gif",
			"lib/fancybox/source/fancybox_loading.gif",
			"lib/fancybox/source/fancybox_loading@2x.gif",
			"lib/fancybox/source/fancybox_overlay.png",
			"lib/fancybox/source/fancybox_sprite.png",
			"lib/fancybox/source/fancybox_sprite@2x.png"
		],
		"dest" : "wp-content/themes/bars2013/fancybox/"
	},

	"vendor": {
		"src" : "vendor/**/*",
		"dest" : "wp-content/themes/bars2013/"
	}
